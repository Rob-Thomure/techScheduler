<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="250.0" prefWidth="380.0"  xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ReportByAppointmentTypeController">
    <children>
        <VBox alignment="CENTER" prefHeight="250.0" prefWidth="380.0">
            <children>
                <VBox alignment="CENTER" prefHeight="50.0" prefWidth="380.0">
                    <children>
                        <Label text="Number of Appointment Types By Month Report">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Label>
                    </children>
                </VBox>
                <TableView fx:id="reportTable" maxWidth="250.0" prefHeight="153.0" prefWidth="336.0">
                    <columns>
                        <TableColumn fx:id="monthCol" prefWidth="92.0" text="Month" />
                        <TableColumn fx:id="numberApptTypeCol" prefWidth="157.0" text="# of Appointment Types" />
                    </columns>
                </TableView>
                <VBox alignment="CENTER_RIGHT" prefHeight="49.0" prefWidth="380.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#onActionBack" text="Back" />
                    </children>
                    <padding>
                        <Insets right="10.0" />
                    </padding>
                </VBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
